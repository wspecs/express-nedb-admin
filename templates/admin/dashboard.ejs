                    <div class="text-right text-green">
                      <i class="fe fe-chevron-up"></i>
                    </div>
                    <div class="h1 m-0"><%- info.value %></div>
                    <div class="text-muted mb-4"><%- info.name %></div>
                  </div>
                </div>
              </div>
              <% } %>

